Algoritmo "CONT-10-C.REPITA"

Var
   CONT : INTEIRO


Inicio
      CONT <- 1

    ESCREVAL ("=====================")
    ESCREVAL(" CONTADOR MATEMÁTICO ")
    ESCREVAL ("=====================")
    
    ESCREVAL("VAMOS CONTAR DE 0 A 10")
    
    REPITA
          ESCREVAL(CONT)
          CONT <- CONT + 1
    ATE (CONT > 10)
    


Fimalgoritmo